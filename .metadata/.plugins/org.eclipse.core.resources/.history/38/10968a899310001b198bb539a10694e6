package chap03_searc;

import java.util.Scanner;

//p117 ?—°?Šµë¬¸ì œ 05. binSearch ë©”ì„œ?“œ ?ž‘?„±?•˜ê¸?
public class SearchEx05 {
	
	static int binSearchX(int a[], int n, int key) { 
		
		int l = 0; 
		int r = n-1; 	
		
		do {
			int c = (l+r)/ 2;
			
			if(a[c] == key) {
				for(; c < l ; c--)	//key?? ê°™ì? ë§? ?•ž?˜ ?š”?†Œë¥? ì°¾ìŠµ?‹ˆ?‹¤. 
					if(a[c-1] < key)
						break;
				return c; //ê²??ƒ‰?„±ê³µì‹œ
			}
			
			else if(a[c] > key) 
				l = c + 1;
			
			else 
				r = c - 1;
			
		} while(l <= r);
		
		
		return -1;
	}
	
	
	
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in); 
		int num = sc.nextInt();
		int x[] = new int[num];
	}

}
