package javaStudyNote;

public class StaticEx {
	
	static int a = 0;  //클래스 필드
	
	int b = 0; //인스턴스 필드
	
	

	public static void main(String[] args) {
	
		StaticEx ex01 = new StaticEx(); // 첫번째
		StaticEx ex02 = new StaticEx(); // 두번째
		
		ex01.a++; //클래스 필드 a를 1 증가시킴
		ex01.b++; //인스턴스 필드 b를 1 증가시킴
		
		System.out.println(ex02.a); // 두번째 ex02의 클래스 필드 출력
		System.out.println(ex02.b); // 두번째 ex02의 
	}

}
