package studyNote;

public class StaticEx {
	
	static int a = 0;  // 정적 변수
	
	int b = 0; //인스턴스 변수
	
	

	public static void main(String[] args) {
	
		StaticEx ex01 = new StaticEx(); //
		StaticEx ex02 = new StaticEx(); // 
		
		ex01.a++; //
		ex01.b++; //
		
		System.out.println(ex02.a); //
		System.out.println(ex02.b); // 
	}

}
